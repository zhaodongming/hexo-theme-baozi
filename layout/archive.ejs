
<section>
    <div  class="container">
        <div class="main">
            <div class="timeline timeline-wrap">
                <div>
                    <% var last;page.posts.each(function(post, i) {
                        var year = post.date.year();
                    if (last != year) {
                        last = year; %>
                </div>
                <div class="archivesBox"><div class="timeline-row-major">
                        <h1 class="title"><%= year %></h1>
                    </div>
                    <% } %>
                    <div class="timeline-row">
                        <div class="content">

                            <div class="article-meta">
                                <% if (post.date) { %>
                                <div class="article-date">
                                    <a href="<%- url_for(post.path) %>">
                                        <time datetime="<%= date_xml(post.date) %>" itemprop="datePublished"><%= date(post.date) %></time>
                                    </a>
                                </div>
                                <% } %>
                                <h1 itemprop="name">
                                    <a class="timeline-article-title" href="<%- url_for(post.link||post.path) %>" target="_blank" itemprop="url"><%= post.title %></a>
                                </h1>
                                <% if (post.tags && post.tags.length) { %>
                                <div class="article-tag">
                                    <%- list_tags(post.tags, { show_count: false, style: 'link',separator:"/" }) %>
                                </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                    <% }); %>
                </div>
            </div>
            <% if (page.total > 1){%>
            <%- partial('page') %>
            <% } %>

        </div>
        <aside>
            <%- partial('_partial/wx') %>
            <%- partial('_partial/class') %>
            <%- partial('_partial/label') %>
            <%- partial('_partial/friendshipLink') %>
        </aside>

    </div>
</section>
<%- partial('_partial/backtop') %>